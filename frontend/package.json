{
    "name": "featherpanel-frontend",
    "version": "0.0.1",
    "private": false,
    "publisher": "MythicalSystems",
    "description": "Frontend for FeatherPanel - The next generation server management panel.",
    "author": {
        "name": "NaysKutzu",
        "email": "ghermancassian2008@gmail.com",
        "url": "https://nayskutzu.xyz"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/mythicalltd/featherpanel"
    },
    "bugs": {
        "url": "https://github.com/mythicalltd/featherpanel/issues"
    },
    "homepage": "https://www.mythical.systems",
    "license": "MIT",
    "type": "module",
    "keywords": [
        "featherpanel",
        "vue",
        "vite",
        "server",
        "dashboard",
