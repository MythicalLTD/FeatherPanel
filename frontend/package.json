{
	"name": "mythicalpanel-frontend",
	"version": "0.0.1",
	"private": false,
	"publisher": "MythicalSystems",
	"description": "Frontend for MythicalPanel - The next generation server management panel.",
	"author": {
		"name": "NaysKutzu",
		"email": "ghermancassian2008@gmail.com",
		"url": "https://nayskutzu.xyz"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/mythicalltd/mythicalpanel"
	},
	"bugs": {
		"url": "https://github.com/mythicalltd/mythicalpanel/issues"
	},
	"homepage": "https://www.mythical.systems",
	"license": "MIT",
	"type": "module",
	"keywords": [
		"mythicalpanel",
		"vue",
		"vite",
		"server",
		"dashboard",
		"panel"
	],
	"funding": {
		"type": "github",
		"url": "https://github.com/sponsors/nayskutzu"
	},
	"engines": {
		"node": ">=22.0.0",
		"yarn": ">=1.22.0"
	},
	"scripts": {
		"dev": "vite --host",
		"build": "NODE_OPTIONS=--max_old_space_size=4096 run-p type-check \"build-only {@}\" --",
		"build-win": "run-p type-check \"build-only {@}\" --",
		"build-debug": "vite build --mode development --no-minify",
		"preview": "vite preview",
		"build-only": "vite build",
		"type-check": "vue-tsc --build --force",
		"lint": "run-p format lint-fix",
		"lint-fix": "eslint . --fix",
		"format": "prettier --log-level silent --write src/",
		"test": "echo \"No tests yet\" && exit 0",
		"clean": "rm -rf dist node_modules",
		"check": "yarn lint && yarn type-check",
		"ui:add": "npx shadcn@latest add"
	},
	"dependencies": {
		"@modyfi/vite-plugin-yaml": "^1.1.1",
		"@monaco-editor/loader": "^1.5.0",
		"@tailwindcss/vite": "^4.1.11",
		"@tanstack/vue-table": "^8.21.3",
		"@tsconfig/node22": "^22.0.2",
		"@types/node": "^24.1.0",
		"@vitejs/plugin-vue": "^6.0.0",
		"@vitejs/plugin-vue-jsx": "^5.0.1",
		"@vue/tsconfig": "^0.7.0",
		"@vueuse/core": "^13.5.0",
		"@vueuse/sound": "^2.1.3",
		"axios": "^1.11.0",
		"chart.js": "^4.5.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"date-fns": "^4.1.0",
		"eslint": "^9.31.0",
		"eslint-config-prettier": "^10.1.8",
		"lucide-vue-next": "^0.525.0",
		"monaco-editor": "^0.52.2",
		"monaco-editor-vue3": "^0.1.10",
		"npm-run-all2": "^8.0.4",
		"pinia": "^3.0.3",
		"prettier": "^3.6.2",
		"qrcode": "^1.5.4",
		"reka-ui": "^2.4.0",
		"sweetalert2": "^11.22.2",
		"tailwind-merge": "^3.3.1",
		"tailwindcss": "^4.1.11",
		"terser": "^5.43.1",
		"tw-animate-css": "^1.3.5",
		"typescript": "~5.8.3",
		"typescript-eslint": "^8.38.0",
		"vite": "7.0.4",
		"vite-plugin-vue-devtools": "^7.7.7",
		"vue": "^3.5.17",
		"vue-i18n": "^11.1.10",
		"vue-qrcode": "^2.2.2",
		"vue-router": "^4.5.1",
		"vue-sweetalert2": "^5.0.11",
		"vue-tsc": "^3.0.1",
		"vue-turnstile": "^1.0.11"
	},
	"devDependencies": {
		"@types/axios": "^0.14.4",
		"eslint-plugin-vue": "^10.3.0",
		"globals": "^16.3.0",
		"vue-eslint-parser": "^10.2.0"
	}
}
