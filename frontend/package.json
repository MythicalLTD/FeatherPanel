    "funding": {
        "type": "github",
        "url": "https://github.com/sponsors/nayskutzu"
    },
    "engines": {
        "node": ">=22.0.0"
    },
    "scripts": {
        "dev": "vite --host",
        "build": "NODE_OPTIONS=--max_old_space_size=4096 run-p type-check \"build-only {@}\" --",
        "build-win": "run-p type-check \"build-only {@}\" --",
        "build-debug": "vite build --mode development --no-minify",
        "preview": "vite preview",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "lint": "run-p format lint-fix",
        "lint-fix": "eslint . --fix",
        "format": "prettier --log-level silent --write src/",
        "test": "echo \"No tests yet\" && exit 0",
        "clean": "rm -rf dist node_modules",
        "check": "pnpm lint && pnpm type-check",
        "ui:add": "npx shadcn-vue@latest add select"
    },
    "dependencies": {
        "@eslint/js": "^8.57.1",
        "@modyfi/vite-plugin-yaml": "^1.1.1",
        "@monaco-editor/loader": "^1.5.0",
        "@tailwindcss/vite": "^4.1.12",
        "@tanstack/vue-table": "^8.21.3",
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^24.3.0",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vitejs/plugin-vue-jsx": "^5.1.1",
        "@vue/tsconfig": "^0.7.0",
        "@vueuse/core": "^13.9.0",
        "@vueuse/sound": "^2.1.3",
        "ansi-to-html": "^0.7.2",
        "axios": "^1.11.0",
        "chart.js": "^4.5.0",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "eslint": "^9.34.0",
        "eslint-config-prettier": "^10.1.8",
        "lucide-vue-next": "^0.539.0",
        "monaco-editor": "^0.52.2",
        "monaco-editor-vue3": "^0.1.10",
        "npm-run-all2": "^8.0.4",
        "pinia": "^3.0.3",
        "prettier": "^3.6.2",
        "qrcode": "^1.5.4",
        "reka-ui": "^2.5.0",
        "tailwind-merge": "^3.3.1",
        "tailwindcss": "^4.1.12",
        "tw-animate-css": "^1.3.7",
        "typescript": "~5.9.2",
        "typescript-eslint": "^8.42.0",
        "vaul-vue": "^0.4.1",
        "vee-validate": "^4.15.1",
        "vite": "7.1.1",
        "vite-plugin-vue-devtools": "^8.0.1",
        "vue": "^3.5.21",
        "vue-i18n": "^11.1.11",
        "vue-qrcode": "^2.2.2",
        "vue-router": "^4.5.1",
        "vue-toastification": "2.0.0-rc.5",
        "vue-tsc": "^3.0.6",
        "vue-turnstile": "^1.0.11",
        "vue-web-terminal": "^3.4.1"
    },
    "devDependencies": {
        "eslint-plugin-vue": "^10.4.0",
        "globals": "^16.3.0",
        "vue-eslint-parser": "^10.2.0"
    }
